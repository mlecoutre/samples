<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="143.0" prefWidth="250.0" xmlns:fx="http://javafx.com/fxml" fx:controller="org.mat.sample.event.ui.AboutController">
  <children>
    <Label text="About ..." textFill="#404040">
      <font>
        <Font name="System Bold" size="14.0" />
      </font>
      <VBox.margin>
        <Insets left="30.0" fx:id="x2" />
      </VBox.margin>
    </Label>
    <GridPane>
      <children>
        <Label text="1.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <Label fx:id="version" text="Version" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <font>
            <Font name="System Bold" size="11.0" fx:id="x1" />
          </font>
        </Label>
        <Label fx:id="author" font="$x1" text="Author" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <Label fx:id="application" font="$x1" text="Application" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <Label fx:id="authorName" text="mlecoutre" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label text="Unknown" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <VBox.margin>
        <Insets left="10.0" />
      </VBox.margin>
    </GridPane>
    <Button fx:id="okButton" defaultButton="true" mnemonicParsing="false" onAction="#handleOkButton" text="Ok" VBox.margin="$x2" />
  </children>
</VBox>
